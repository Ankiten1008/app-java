node{
  stage('GIT Checkout'){
    checkout([$class: 'GitSCM',
                branches: [[name: '*/master' ]],
                extensions: scm.extensions,
                userRemoteConfigs: [[
                    url: 'https://github.com/Ankiten1008/app-java.git',
                    credentialsId: 'gitCredentials'
                ]]
            ])
    }
 }
 
  
